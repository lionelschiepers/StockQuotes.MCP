{
  "name": "mcp-server-stockquotes",
  "version": "1.0.0",
  "description": "MCP Server for fetching stock quotes from Yahoo Finance",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mcp-server-stockquotes": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "node dist/index.js",
    "start:stdio": "node dist/index.js --transport stdio",
    "start:http": "node dist/index.js --transport http",
    "dev": "tsx watch src/index.ts --transport stdio",
    "clean": "rm -rf dist",
    "inspect": "tsc && npx -y @modelcontextprotocol/inspector node dist/index.js",
    "prepare": "husky"
  },
  "keywords": [
    "mcp",
    "stock-quotes",
    "yahoo-finance",
    "mcp-server",
    "ai-tools"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "yahoo-finance2": "^3.11.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.6",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "npm-check-updates": "^19.3.1",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
