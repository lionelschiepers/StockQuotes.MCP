{
  "name": "stockquotes-mcp",
  "version": "1.0.8",
  "description": "MCP Server for fetching stock quotes from Yahoo Finance",
  "author": "Lionel Schiepers <lionel_schiepers@hotmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lionelschiepers/StockQuotes.MCP.git"
  },
  "bugs": {
    "url": "https://github.com/lionelschiepers/StockQuotes.MCP/issues"
  },
  "homepage": "https://github.com/lionelschiepers/StockQuotes.MCP#readme",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "bin": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "node dist/index.js",
    "start:stdio": "node dist/index.js --transport stdio",
    "start:http": "node dist/index.js --transport http --http-host localhost",
    "dev": "tsx watch src/index.ts --transport stdio",
    "clean": "rm -rf dist",
    "inspect": "tsc && npx -y @modelcontextprotocol/inspector node dist/index.js",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "publish": "npm publish",
    "publish:dry": "npm publish --dry-run"
  },
  "keywords": [
    "mcp",
    "stock-quotes",
    "yahoo-finance",
    "mcp-server",
    "ai-tools",
    "financial data",
    "finance",
    "api"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "node-cache": "^5.1.2",
    "winston": "^3.19.0",
    "yahoo-finance2": "^3.11.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.9",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "npm-check-updates": "^19.3.1",
    "prettier": "^3.8.0",
    "semantic-release": "^25.0.2",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
